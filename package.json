{
  "name": "TSearch",
  "version": "1.0.0",
  "description": "all in one app that can search and download torrents",
  "main": "index.js",
  "scripts": {
    "start": "npx cross-env TARGET=electron npm run start:dev",
    "build": "npx cross-env TARGET=electron npm run start:prod && npm run build:electron",
    "start:electron": "npx cross-env NODE_ENV=development electron .",
    "build:electron": "npx cross-env NODE_ENV=production electron .",
    "start:web": "npx cross-env TARGET=web npm run start:dev",
    "start:dev": "npx cross-env NODE_ENV=development npx webpack-dev-server --config ./config/webpack/index.js --dev",
    "start:prod": "npx cross-env NODE_ENV=production npx webpack --config ./config/webpack/index.js",
    "pack:win": "npx electron-packager . TSearch --platform=win32 --asar --overwrite --icon ./resources/icon.png",
    "pack:mac": "npx electron-packager . TSearch --platform=darwin --asar --overwrite --icon ./resources/icon.png",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Moti Shriki, Koby Bass, Orel Shriki",
  "license": "GPL-3.0",
